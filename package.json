{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha --require @babel/register src/tests/index.js",
    "cover": "npm test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "start": "babel-node src/index.js",
    "dev": "nodemon src/index.js --exec babel-node --presets @babel/preset-env",
    "scss": "npm-sass templates/sass/style.scss  templates/css/style.css",
    "db:migrate": "sequelize db:migrate",
    "dbtest:migrate": "NODE_ENV=test sequelize db:migrate",
    "dbtest:seed":"NODE_ENV=test sequelize db:seed:all"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "body-parser": "^1.18.3",
    "cheke": "^1.0.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "extend": "^3.0.2",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.1",
    "mpath": "^0.5.1",
    "npm-sass": "^2.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^5.7.5",
    "sequelize-test-helpers": "^1.1.0",
    "slug": "^1.1.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
